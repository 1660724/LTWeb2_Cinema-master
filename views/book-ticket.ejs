<div id="divBookTicket">
    <div id="bookTicketContent">
        <span id="bookingTitleText" class="movie-title">
            <i class='fas fa-film sidebar-icon' style='font-size:30px'></i> Now Showing</span>
        <hr style="border: 0.5px solid skyblue" />
        <%- include ./sidebar.ejs %>
    </div>
</div>

<div class="modal fade movie-info" id="booked-tickets" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header" style="border: 0">
                <img src="images/wings_2.png" style="height: 30px; width: 30px;" />
                <span style="color:skyblue">Wings
                    <span style="font-weight:bold; color: white">Cinema</span>
                </span>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div id="booking-alert" style="padding: 10px; padding-top: 0; padding-bottom: 5px"></div>
            <div class="modal-body" id="booked-tickets-body"></div>
        </div>
    </div>
</div>